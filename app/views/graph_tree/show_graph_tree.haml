%section.admin


  %h3 GraphTree#show_graph_tree
  %p
    Find me in app/views/graph_tree/show_graph_tree.haml</p>

  %h3 show_graph_tree
  %h3 Страница Просмотра и управления Графом дерева Пользователя
  %p
    @session_id: #{@session_id.inspect}

  %div
    - if user_signed_in?
      %p
        current_user.id: #{current_user.id.inspect}
      %p
        @profiles_tree_arr_len: #{@profiles_tree_arr_len.inspect}

      %div
        %h3 Отображение Древа Пользователя в табличной форме
        #{render :partial => 'main/main_display_tree'}

      %div
        %h3 Отображение Древа Пользователя в виде Графа
        #{render :partial => 'graph_tree/canvas_tree/main'}

    - else
      %h4 No Listing tree - Юзер не зареген!




  %h4 Возврат на Главную страницу:  #{link_to "Главная -->", main_page_path}

