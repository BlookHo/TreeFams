var rollbackId = "<%= params[:rollback_id] %>";
var rollbackDate = "<%= params[:rollback_date] %>";
if(typeof rollbackDate != "undefined" && rollbackDate != null && rollbackDate.length > 0){
    var textStr = '<br>' + 'Return tree to chosen date state = ' + rollbackDate + ' - completed!' + '<br>';
    $('.log_id').empty();
    $('.point h2').empty().before().append(textStr).fadeOut(3000);
    setTimeout("window.location.href='<%= home_path %>';",2000);
}

$("#table tr").click(function(){
    $(this).addClass('selected').siblings().removeClass('selected');
    var value=$(this).find('td:first').html();
    alert(value);
});

$('.ok').on('click', function(e){
    alert($("#table tr.selected td:first").html());
});


