$('.search-path-table').hide();
$("#search-path-table-<%= @path_id %>").show();
$("#search-circle-path-table-<%= @path_id %>").animate({
    left: "-=60",
    opacity: "0"
}, 500, function() {
  $('#search-path-table-<%= @path_id %>').html("<%= j render 'main/search_circle_path_table', circle: @circle, author: @author, path_id: @path_id %>");
});
