base: &base
  adapter: postgresql
  username: db_user
  password: db_password
  encoding: utf8
  reconnect: false
  pool: 25
  timeout: 10000
  host: 127.0.0.1

production:
  database: weafam_rails_production
  <<: *base

development:
  database: weafam_rails_development
  <<: *base

test:
  database: weafam_rails_test
  <<: *base
